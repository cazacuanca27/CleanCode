@model IEnumerable<shanuMVCUserRoles.Models.HolidayViewModel>

@{
    ViewBag.Title = "Approved";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Approved</h2>


<div class="table">


    @foreach (var item in Model)
    {
        <div class="form-style-8" style="font-family: 'Open Sans Condensed', arial, sans; opacity: 1 ;width: 500px; animation-fill-mode:backwards;   padding: 30px;    background: #FFFFFF;    margin: 50px auto;    box-shadow: 0px 0px 15px rgba(66, 155, 36, 0.52);    -moz-box-shadow: 0px 0px 15px rgba(66, 155, 36, 0.52);    -webkit-box-shadow:  0px 0px 15px rgba(66, 155, 36, 0.52);">

            <p>
                <b>Team Leader Name</b> :
                @Html.DisplayFor(modelItem => item.TeamLeaderName)
            </p>
            <p>
                <b>@Html.DisplayNameFor(model => model.StartDate)</b> :
                @Html.DisplayFor(modelItem => item.StartDate)
            </p>
            <p>
                <b>@Html.DisplayNameFor(model => model.EndDate)</b> :
                @Html.DisplayFor(modelItem => item.EndDate)
            </p>
            <p>
                <b>@Html.DisplayNameFor(model => model.DaysOff)</b> :
                @Html.DisplayFor(modelItem => item.DaysOff)
            </p>
            <p>
                <b>Team Leader Email</b> :
                @Html.DisplayFor(modelItem => item.TLEmail)
            </p>

        </div>


    }

</div>

